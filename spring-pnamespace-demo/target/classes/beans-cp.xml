<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="myorg" class="com.radostin.springdemo.domain.Organization"
		 p:companyName="BMW" p:yearOfIncorporation="1929" p:address-ref="address"/>

	<bean id="address" class="com.radostin.springdemo.domain.Address"
		p:street="Studentska 13" p:city="Rousse" p:zipCode="7000"/>

	
</beans>
